<% @fellowers.each do |user| %>
  <div class="fellow-list span16" id="fellow_<%= user.id %>" onclick="$('#edit_fellow_fellow_<%= user.id %>').toggle();">
      <div class="span4 padding-left-20"><%= user.username %></div>
      <div class="span4 padding-left-20"><%= user.permission %></div>
      <div class="span6 padding-left-20"><%= user.accessability_type %></div>
    </div>
    <div class="clear-half"></div>
    <div class="edit_user" id="edit_fellow_fellow_<%= user.id %>" style="display: none;">
        <%= render :partial => 'settings/shared/edit_fellow_form', :locals => {:fellower => user} %>
    </div>
<% end %>

